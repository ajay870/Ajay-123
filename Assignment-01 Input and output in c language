    printf("Hello Students");
    return 0;
}  // write a program to print Hello Students on the screen.

//Write a program to print Hello in the first line and Students in the second line.
#include<stdio.h>
int main(){
    printf("Hello \n Students");
    return 0;
}

// write a program to print "MySirG" on the screen.(Remember to print double quotes)
#include<stdio.h>
int main(){
    printf("\"MySirG \" ");
    return 0;
}

//WAP to find the area of the circle.
#include<stdio.h>
int main(){
    int r,pi=3.14, area;
    printf("Enter the radius of the circle");
    scanf("%d",&r);
    area= pi*r*r;
    printf("The area of the circle is %d",area);
    return 0;
}//Expected output format-"Area of circle is A having the radious R". Replace A with area 
//& R with radius

//WAP to print the length of string using printf function.
#include<stdio.h>
int main(){
    char = "KAJAL";
    printf("The length of string is %d",sizeof(char));
    return 0;
}

