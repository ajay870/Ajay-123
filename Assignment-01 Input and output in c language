//WAP to print "\\" on the screen.

//WAP to take date as an input in below given format and convert the date format and display the result as given below.
#include<stdio.h>
int main(){
    int date,month,year;
    printf("Enter the date, month and year");
    scanf("%d",&date,&month,&year);
    printf("date-%d, month-%d, year-%d",date,month,year);
} 

#include<stdio.h>
int main(){
    int HH,MM;
    printf("Enter the hour and minute");
    scanf("%d:%d",&HH,&MM);
    printf("%d hour and %d minutes",HH,MM);
}
// Find output of below code.
#include<stdio.h>
int main(){
    int x = printf("ineuron");
    printf("%d",&x);
    return 0;
}